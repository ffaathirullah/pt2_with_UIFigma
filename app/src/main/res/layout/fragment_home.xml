                android:textColor="#FFFFFF"
                android:textSize="34sp"
                app:layout_constraintStart_toStartOf="@+id/imageView12"
                app:layout_constraintTop_toBottomOf="@+id/tv_now" />

            <TextView
                android:id="@+id/tv_derajat4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                android:text="22°C"
                android:textColor="#232121"
                android:textSize="34sp"
                app:layout_constraintStart_toStartOf="@+id/imageView6"
                app:layout_constraintTop_toBottomOf="@+id/tv_now4" />

            <TextView
                android:id="@+id/tv_cerah"
                android:layout_width="155dp"
                android:layout_height="37dp"
                android:layout_marginTop="8dp"
                android:text="Cerah Untuk Beraktifitas"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="@+id/tv_derajat"
                app:layout_constraintHorizontal_bias="0.43"
                app:layout_constraintStart_toStartOf="@+id/tv_derajat"
                app:layout_constraintTop_toBottomOf="@+id/tv_derajat" />


        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

            <ImageView
                android:id="@+id/rctangle_1"
                android:layout_width="390sp"
                android:layout_height="102sp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/rectangle_home"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.545"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/icon_gambar1"
                android:layout_width="104dp"
                android:layout_height="68dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/icon_berita_1"
                app:layout_constraintStart_toStartOf="@+id/rctangle_1"
                app:layout_constraintTop_toTopOf="@+id/rctangle_1" />

            <ImageView
                android:id="@+id/rectangle2"
                android:layout_width="390sp"
                android:layout_height="102sp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/rectangle_home"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/rctangle_1"
                app:layout_constraintTop_toBottomOf="@+id/rctangle_1" />

            <ImageView
                android:id="@+id/icon_gambar2"
                android:layout_width="104dp"
                android:layout_height="68dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="16dp"
